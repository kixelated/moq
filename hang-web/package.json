{
	"name": "@kixelated/hang",
	"type": "module",
	"collaborators": [
		"Luke Curley <kixelated@gmail.com>"
	],
	"description": "Web implementation for MoQ utilizing WebAssembly+Typescript",
	"version": "0.1.0",
	"license": "MIT OR Apache-2.0",
	"repository": "github:kixelated/moq",
	"files": [
		"dist",
		"pkg"
	],
	"exports": {
		".": "./dist/index.js"
	},
	"sideEffects": [
