{
	"name": "@kixelated/moq",
	"version": "0.0.1",
	"type": "module",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		},
		"./video": {
			"types": "./dist/element/video.d.ts",
			"import": "./dist/element/video.js"
		},
		"./publish": {
			"types": "./dist/element/publish.d.ts",
			"import": "./dist/element/publish.js"
		},
		"./watch": {
			"types": "./dist/element/watch.d.ts",
			"import": "./dist/element/watch.js"
		},
		"./meet": {
			"types": "./dist/element/meet.d.ts",
			"import": "./dist/element/meet.js"
		}
	},
	"sideEffects": [
		"./dist/element/video.js",
		"./dist/element/publish.js",
		"./dist/element/watch.js",
		"./dist/element/meet.js",
		"./dist/rust.js",
		"./dist/snippets/*"
	],
	"files": ["dist", "README.md", "LICENSE*"],
	"scripts": {
		"build": "npm-run-all --sequential build:wasm build:ts",
		"build:wasm": "wasm-pack build --out-dir dist . --release --scope kixelated --out-name rust && rm dist/.gitignore dist/README.md dist/package.json",
		"build:ts": "tsc && tsc-alias",
		"dev": "npm-run-all --parallel dev:wasm dev:ts dev:tsa",
		"dev:wasm": "chokidar 'src/**/*.rs' Cargo.toml Cargo.lock --initial -i dist -i node_modules -c 'wasm-pack build --out-dir dist . --scope kixelated --out-name rust'",
		"dev:ts": "tsc --watch",
		"dev:tsa": "tsc-alias --watch"
	},
	"dependencies": {
		"comlink": "^4.4.2"
	},
	"devDependencies": {
		"@types/dom-mediacapture-transform": "^0.1.10",
		"@wasm-tool/wasm-pack-plugin": "^1.5.0",
		"chokidar-cli": "^3.0.0",
		"npm-run-all": "^4.1.5",
		"tsc-alias": "^1.8.10",
		"typescript": "^5.7.3",
		"wasm-pack": "^0.13.1"
	}
}
